{
  "name": "TestRNConfig",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "ios:clean": "cd ios && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "ios:dev": "./scripts/react-native/ios_dev.sh",
    "ios:dev:release": "./scripts/react-native/ios_dev_release.sh",
    "ios:staging": "./scripts/react-native/ios_staging.sh",
    "ios:staging:release": "./scripts/react-native/ios_staging_release.sh",
    "ios:production": "./scripts/react-native/ios_production.sh",
    "ios:production:release": "./scripts/react-native/ios_production_release.sh",
    "android:dev": "./scripts/react-native/android_dev.sh",
    "android:dev:release": "./scripts/react-native/android_dev_release.sh",
    "android:staging": "./scripts/react-native/android_staging.sh",
    "android:staging:release": "./scripts/react-native/android_staging_release.sh",
    "android:production": "./scripts/react-native/android_production.sh",
    "android:production:release": "./scripts/react-native/android_production_release.sh",
    "open-apk": "open ./android/app/build/outputs/apk/",
    "open-bundle": "open ./android/app/build/outputs/bundle/",
    "scripts:permission": "find ./scripts -type f -iname '*.sh' -exec chmod +x {} \\;"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
